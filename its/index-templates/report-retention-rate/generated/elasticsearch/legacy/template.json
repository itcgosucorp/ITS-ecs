{
  "index_patterns": [
    "report_*_*_retention_rate"
  ],
  "mappings": {
    "dynamic": false,
    "properties": {
      "@timestamp": {
        "type": "date"
      },
      "app_code": {
        "ignore_above": 1024,
        "type": "keyword"
      },
      "application_id": {
        "ignore_above": 1024,
        "type": "keyword"
      },
      "d0": {
        "type": "date"
      },
      "rr_d1": {
        "type": "double"
      },
      "rr_d3": {
        "type": "double"
      },
      "rr_d30": {
        "type": "double"
      },
      "rr_d7": {
        "type": "double"
      }
    }
  },
  "order": 1,
  "settings": {
    "index": {
      "codec": "best_compression",
      "mapping": {
        "total_fields": {
          "limit": 1000
        }
      },
      "refresh_interval": "2s"
    }
  }
}
