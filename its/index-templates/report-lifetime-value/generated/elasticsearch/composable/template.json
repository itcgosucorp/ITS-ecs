{
  "_meta": {
    "description": "Sample composable template that includes all ECS fields",
    "ecs_version": "8.9.0"
  },
  "composed_of": [
    "ecs_8.9.0_base",
    "ecs_8.9.0_its_report_lifetime_value"
  ],
  "index_patterns": [
    "report_*_*_lifetime_value"
  ],
  "priority": 1,
  "template": {
    "mappings": {
      "dynamic": false
    },
    "settings": {
      "index": {
        "codec": "best_compression",
        "mapping": {
          "total_fields": {
            "limit": 2000
          }
        }
      }
    }
  }
}
